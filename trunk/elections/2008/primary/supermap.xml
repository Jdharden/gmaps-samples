<?xml version="1.0" encoding="UTF-8" ?>
<Module>
<ModulePrefs
	title="Election Map from Google"
	title_url="http://maps.google.com/decision2008"
	description="View live election results for the 2008 primaries."
	category="politics"
	thumbnail="http://gmaps-samples.googlecode.com/svn/trunk/elections/2008/primary/supermap-thumb.png"
	screenshot="http://gmaps-samples.googlecode.com/svn/trunk/elections/2008/primary/supermap-screenshot.png"
	author="Michael Geary"
	author_affiliation="Google, Inc."
	author_email="elections@google.com"
	author_location="San Jose, CA"
	width="700"
	height="400"
	scaling="false"
	scrolling="false"
>
</ModulePrefs>
<UserPref name="zoom" default_value="3" datatype="enum" display_name="Zoom&#160;level">
	<EnumValue value="2"/>
	<EnumValue value="3"/>
	<EnumValue value="4"/>
	<EnumValue value="5"/>
</UserPref>
<UserPref name="sidebarwidth" default_value="280" datatype="string" display_name="Sidebar&#160;width"/>
<UserPref name="party" default_value="" datatype="enum" display_name="Party">
	<EnumValue value="" display_value="Both"/>
	<EnumValue value="democrat" display_value="Democratic"/>
	<EnumValue value="republican" display_value="Republican"/>
</UserPref>
<UserPref name="twitter" default_value="false" datatype="bool" display_name="Twitter&#160;updates"/>
<UserPref name="youtube" default_value="false" datatype="bool" display_name="YouTube&#160;videos"/>
<Content type="html">
<![CDATA[
	<script type="text/javascript">
		document.write(
			'<script type="text/javascript" src="',
				_IG_GetCachedUrl( 'http://gmaps-samples.googlecode.com/svn/trunk/elections/2008/primary/mapplet.js', { refreshInterval:120 } ),
			'">',
			'<\/script>'
		);
	</script>
]]>
</Content>
</Module>
